@model ICollection<MusicPortal.WEB.Models.ViewModels.MyMusicVM>

<main>
    <div class="container">
        <h2>Мой плейлист</h2>
        <table class="table">
            <thead>
                <tr>
                    <th>Название</th>
                    <th>Исполнитель</th>
                    <th>Жанр</th>
                    <th>Дата релиза</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Musics.Name</td>
                        <td>@item.Author.NickName</td>
                        <td>@item.Musics.Genre</td>
                        <td>@item.Musics.Date</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</main>
